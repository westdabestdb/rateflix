{
  "manifest_version": 2,
  "name": "Rateflix",
  "description": "Get movie ratings from IMDB and more.",
  "version": "1.0",
  "icons": {
    "16": "r1.png",
    "48": "r1.png",
   "128": "r1.png" },
  "content_scripts": [
    {
      "matches": [
        "https://www.netflix.com/browse",
        "https://www.netflix.com/title/*"
      ],
      "js": [
        "rateflix.min.js"
      ],
      "run_at": "document_end"
    }
  ]
}